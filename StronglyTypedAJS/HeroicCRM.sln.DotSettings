	Expression&lt;Func&lt;TModel, IEnumerable&lt;TSubModel&gt;&gt;&gt; property, &#xD;
	string variableName)&#xD;
{&#xD;
	var propertyExpression = ExpressionForInternal(property);&#xD;
	return new AngularNgRepeatHelper&lt;TSubModel&gt;(&#xD;
		Helper, variableName, propertyExpression);&#xD;
}</s:String>
