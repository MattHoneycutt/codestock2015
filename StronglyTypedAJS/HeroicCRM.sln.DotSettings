	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=DCBC392D9CB62345854D1CBEC0E6D430/Text/@EntryValue">public HtmlTag FormGroupFor&lt;TProp&gt;(Expression&lt;Func&lt;TModel, TProp&gt;&gt; property)&#xD;
{&#xD;
	var name = ExpressionHelper.GetExpressionText(property);&#xD;
&#xD;
	var expression = ExpressionForInternal(property);&#xD;
&#xD;
	//Creates &lt;div class="form-group has-feedback"&gt;&#xD;
	var formGroup = new HtmlTag("div")&#xD;
		.AddClasses("form-group");&#xD;
&#xD;
	//Creates &lt;label class="control-label" for="Name"&gt;Name&lt;/label&gt;&#xD;
	var label = new HtmlTag("label")&#xD;
		.AddClass("control-label")&#xD;
		.Attr("for", name)&#xD;
		.Text(name);&#xD;
&#xD;
	//Creates &lt;input ng-model="expression"&#xD;
	//		   class="form-control" name="Name" type="text" &gt;&#xD;
	var input = new HtmlTag("input")&#xD;
		.AddClass("form-control")&#xD;
		.Attr("ng-model", expression)&#xD;
		.Attr("name", name)&#xD;
		.Attr("type", "text");&#xD;
&#xD;
	return formGroup&#xD;
		.Append(label)&#xD;
		.Append(input);&#xD;
}</s:String>
