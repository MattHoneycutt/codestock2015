	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=5FE1D9DDB4BAAF4B9D99F385036EBADC/Text/@EntryValue">@using System&#xD;
@using System.Threading.Tasks&#xD;
@inject IOptions&lt;AppSettings&gt; AppSettings&#xD;
&lt;!DOCTYPE html&gt;&#xD;
&lt;html&gt;&#xD;
    &lt;head&gt;&#xD;
        &lt;meta charset="utf-8" /&gt;&#xD;
        &lt;meta name="viewport" content="width=device-width, initial-scale=1.0" /&gt;&#xD;
        &lt;title&gt;@ViewBag.Title - @AppSettings.Options.SiteTitle&lt;/title&gt;&#xD;
&#xD;
        &lt;link rel="stylesheet" href="~/css/all.min.css" /&gt;&#xD;
    &lt;/head&gt;&#xD;
    &lt;body&gt;&#xD;
        &lt;!-- Navigation --&gt;&#xD;
        &lt;nav class="navbar navbar-default navbar-custom navbar-fixed-top"&gt;&#xD;
            &lt;div class="container-fluid"&gt;&#xD;
                &lt;!-- Brand and toggle get grouped for better mobile display --&gt;&#xD;
                &lt;div class="navbar-header page-scroll"&gt;&#xD;
                    &lt;button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1"&gt;&#xD;
                        &lt;span class="sr-only"&gt;Toggle navigation&lt;/span&gt;&#xD;
                        &lt;span class="icon-bar"&gt;&lt;/span&gt;&#xD;
                        &lt;span class="icon-bar"&gt;&lt;/span&gt;&#xD;
                        &lt;span class="icon-bar"&gt;&lt;/span&gt;&#xD;
                    &lt;/button&gt;&#xD;
                    &lt;a asp-controller="Home" asp-action="Index" class="navbar-brand"&gt;&#xD;
                        try-catch-FAIL&#xD;
                    &lt;/a&gt;&#xD;
                &lt;/div&gt;&#xD;
&#xD;
                &lt;!-- Collect the nav links, forms, and other content for toggling --&gt;&#xD;
                &lt;div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1"&gt;&#xD;
                    &lt;ul class="nav navbar-nav navbar-right"&gt;&#xD;
                        &lt;li&gt;&lt;a asp-controller="Home" asp-action="Index"&gt;Home&lt;/a&gt;&lt;/li&gt;&#xD;
                        &lt;li&gt;&lt;a asp-controller="Archive" asp-action="Index"&gt;Archive&lt;/a&gt;&lt;/li&gt;&#xD;
                        &lt;li&gt;&lt;a asp-controller="Home" asp-action="Contact"&gt;Contact&lt;/a&gt;&lt;/li&gt;&#xD;
                        &lt;li&gt;&lt;a asp-controller="RSS" asp-action="Index"&gt;RSS&lt;/a&gt;&lt;/li&gt;&#xD;
                    &lt;/ul&gt;&#xD;
                &lt;/div&gt;&#xD;
                &lt;!-- /.navbar-collapse --&gt;&#xD;
            &lt;/div&gt;&#xD;
            &lt;!-- /.container --&gt;&#xD;
        &lt;/nav&gt;&#xD;
&#xD;
        &lt;!-- Page Header --&gt;&#xD;
        &lt;!-- Set your background image for this header on the line below. --&gt;&#xD;
        &lt;header class="intro-header"&gt;&#xD;
            &lt;div class="container"&gt;&#xD;
                &lt;div class="row"&gt;&#xD;
                    &lt;div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1"&gt;&#xD;
                        &lt;div class="site-heading"&gt;&#xD;
                            @RenderSection("SiteHeading")&#xD;
                        &lt;/div&gt;&#xD;
                    &lt;/div&gt;&#xD;
                &lt;/div&gt;&#xD;
            &lt;/div&gt;&#xD;
        &lt;/header&gt;&#xD;
&#xD;
        &lt;!-- Main Content --&gt;&#xD;
        &lt;div class="container"&gt;&#xD;
            @RenderBody()&#xD;
        &lt;/div&gt;&#xD;
&#xD;
        &lt;footer&gt;&#xD;
            &lt;div class="container"&gt;&#xD;
                &lt;div class="row"&gt;&#xD;
                    &lt;div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1"&gt;&#xD;
                        &lt;ul class="list-inline text-center"&gt;&#xD;
                            &lt;li&gt;&#xD;
                                &lt;a href="#"&gt;&#xD;
                                    &lt;span class="fa-stack fa-lg"&gt;&#xD;
                                        &lt;i class="fa fa-circle fa-stack-2x"&gt;&lt;/i&gt;&#xD;
                                        &lt;i class="fa fa-twitter fa-stack-1x fa-inverse"&gt;&lt;/i&gt;&#xD;
                                    &lt;/span&gt;&#xD;
                                &lt;/a&gt;&#xD;
                            &lt;/li&gt;&#xD;
                            &lt;li&gt;&#xD;
                                &lt;a href="#"&gt;&#xD;
                                    &lt;span class="fa-stack fa-lg"&gt;&#xD;
                                        &lt;i class="fa fa-circle fa-stack-2x"&gt;&lt;/i&gt;&#xD;
                                        &lt;i class="fa fa-facebook fa-stack-1x fa-inverse"&gt;&lt;/i&gt;&#xD;
                                    &lt;/span&gt;&#xD;
                                &lt;/a&gt;&#xD;
                            &lt;/li&gt;&#xD;
                            &lt;li&gt;&#xD;
                                &lt;a href="#"&gt;&#xD;
                                    &lt;span class="fa-stack fa-lg"&gt;&#xD;
                                        &lt;i class="fa fa-circle fa-stack-2x"&gt;&lt;/i&gt;&#xD;
                                        &lt;i class="fa fa-github fa-stack-1x fa-inverse"&gt;&lt;/i&gt;&#xD;
                                    &lt;/span&gt;&#xD;
                                &lt;/a&gt;&#xD;
                            &lt;/li&gt;&#xD;
                        &lt;/ul&gt;&#xD;
                        &lt;p class="copyright text-muted"&gt;Copyright &amp;copy; @AppSettings.Options.SiteTitle @DateTime.Now.Year&lt;/p&gt;&#xD;
                    &lt;/div&gt;&#xD;
                &lt;/div&gt;&#xD;
            &lt;/div&gt;&#xD;
        &lt;/footer&gt;&#xD;
&#xD;
        @RenderSection("scripts", required: false)&#xD;
    &lt;/body&gt;&#xD;
&lt;/html&gt;</s:String>
